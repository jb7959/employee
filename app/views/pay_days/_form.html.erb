<%= simple_form_for(@pay_day) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :emp_id %>
    <%= f.input :name %>
    <%= f.input :payment_type %>
    <%= f.input :pay %>
    <%= f.input :lastPayday %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
